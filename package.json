    "db:restart": "docker-compose restart postgres",
    "db:seed": "ts-node src/seed/seed.ts",
    "db:reset": "docker-compose down -v && docker-compose up -d postgres && sleep 5 && npm run db:seed",
    "db:logs": "docker-compose logs -f postgres",
    "pgadmin": "docker-compose up -d pgadmin"
